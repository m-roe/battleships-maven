              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <language>java</language>
            <body>// first cell in a field row has to have a title (e.g. 1)&#xD;
		setRowHeader(rowIdx);&#xD;
		setRowCells(rowIdx);&#xD;
		Printer.printHorizontalSeparator(this.fieldSize);&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0LCDJSaaEeufV6DUtySNPg" name="setRowHeader" specification="_0IeeYCaaEeufV6DUtySNPg">
            <ownedParameter xmi:id="_0LCDJiaaEeufV6DUtySNPg" name="rowIdx">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <language>java</language>
            <body>// Row titles are subsequent numbers&#xD;
		String rowHeaderTitle = transform2RowHeaderTitle(rowIdx);&#xD;
		Printer.printRowTitle(rowHeaderTitle);&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0LCDJyaaEeufV6DUtySNPg" name="transform2RowHeaderTitle" specification="_0IfFcCaaEeufV6DUtySNPg">
            <ownedParameter xmi:id="_0LCDKCaaEeufV6DUtySNPg" name="rowIdx">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
