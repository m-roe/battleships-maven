          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0LBcESaaEeufV6DUtySNPg" name="setupBattleFieldMatrix" specification="_0IcCICaaEeufV6DUtySNPg">
            <language>java</language>
            <body>for(int rowIdx = 0; rowIdx &lt; this.actualFields; rowIdx++) {&#xD;
			setBattleFieldRows(rowIdx);&#xD;
		}&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0LBcEiaaEeufV6DUtySNPg" name="setBattleFieldRows" specification="_0IcpMCaaEeufV6DUtySNPg">
