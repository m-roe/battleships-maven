              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            </ownedParameter>
            <language>java</language>
            <body>String rowHeaderStr = Integer.toString(rowIdx);&#xD;
		return rowHeaderStr;&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0LCqMCaaEeufV6DUtySNPg" name="setRowCells" specification="_0IfsgCaaEeufV6DUtySNPg">
            <ownedParameter xmi:id="_0LCqMSaaEeufV6DUtySNPg" name="rowIdx">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <language>java</language>
            <body>for(int colIdx = 1; colIdx &lt; this.actualFields; colIdx++) {&#xD;
			// check if the cell contains a ship&#xD;
			String symbol = this.fleet.getShipSymbol(rowIdx, colIdx);&#xD;
			if(symbol != null) {&#xD;
				Printer.printFilledCell(symbol);&#xD;
			} else {&#xD;
				Printer.printEmptyCell();&#xD;
			}&#xD;
		}&#xD;
	</body>
