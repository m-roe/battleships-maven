          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0K8jkiaaEeufV6DUtySNPg" name="buildBattleshipFleet" specification="_0IAkUCaaEeufV6DUtySNPg">
            <language>java</language>
            <body>battleships = new ArrayList&lt;Battleship>();&#xD;
		for(int shipNoIdx = 0; shipNoIdx &lt; battleshipNumber; shipNoIdx++) {&#xD;
			int[][] positions = setBattleshipPosition();&#xD;
			Battleship newShip = new Battleship(BattleshipsConfig.DEBUGGER, positions);&#xD;
			this.fleetPositionMatrix[positions[0][0]][positions[0][1]] = newShip;&#xD;
			battleships.add(newShip);&#xD;
		}&#xD;
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0K8jkyaaEeufV6DUtySNPg" name="checkHitShip" specification="_0IBLYCaaEeufV6DUtySNPg">
            <ownedParameter xmi:id="_0K8jlCaaEeufV6DUtySNPg" name="rowIdx">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
